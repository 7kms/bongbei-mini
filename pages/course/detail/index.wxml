<view>
    <view class="container">
        <view class="item" wx:if="{{info}}" wx:for="{{info.sections}}" wx:key="{{index}}">
            <view wx:if="{{item.desc}}">{{item.desc}}</view>
            <image wx:if="{{item.picture}}" class="picture" src="{{imagePrefix + item.picture}}"></image>
        </view>
    </view>
</view>
  