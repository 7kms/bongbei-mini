.item{
  margin-bottom: 25rpx;  
  background-color: #fff;
}
.item .title{
    height: 60rpx;
    line-height: 60rpx;
    background-image: linear-gradient(-114deg, #FDBDD2 0%, #F1F1FF 51%, #9EEAFF 100%);
}
.item .cover{
    width: 100%;
    height: 400rpx;
}
.item .line{
    margin: 25rpx 25rpx 0;
    height: 1px;
    background-color: #E7E7E7;
}