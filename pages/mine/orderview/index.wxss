page{
    padding-bottom: 88rpx;
    background: #FBFBFB;
}
.order-number{
    height: 80rpx;
    line-height: 80rpx;
    padding: 0 45rpx;
    color: #000;
    background-color:  #D4ECF2;
    display: flex;
    justify-content: space-between;
    align-items: center;
}
.item{
    padding: 0 25rpx;
    background-color: #fff;
    margin-bottom: 25rpx;
}
.item .item-dfn{
    padding: 0 10rpx;
    display: flex;
    align-items: center;
    justify-content: space-between;
    height: 80rpx;
    border-bottom: 1px solid  #E7E7E7;
}
.item .item-dfn:last-child{
    border: none;
}
