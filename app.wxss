/**app.wxss**/
page{
  font-family: PingFangSC-Regular;
}
.container {
  box-sizing: border-box;
  
  font-size: 13px;
  color: #95A1A7;
} 
.text-center{
  text-align: center;
}
.text-left{
  text-align: left;
}
.text-right{
  text-align: right;
}
.text-one-line{
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.icon-circle{
  display: block;
  width: 32rpx;
  height: 32rpx;
  border: 1px solid #686E71;
  border-radius: 50%;
}

.fixed-footer{
  position: fixed;
  display: flex;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 88rpx;
  line-height: 88rpx;
  color: #fff;
  font-size: 36rpx;
  text-align: center;
}
.address-list-footer .left{
  width: 270rpx;
  background-color: #91D2E4;
}
.address-list-footer .right{
  flex: 1;
  background-color: #FCBBD0;
}
.product-footer .left{
  width: 50%;
  background-color: #91D2E4;
}
.product-footer .right{
  flex: 1;
  background-color: #FCBBD0;
}
.list-empty{
  display: flex;
  height: 60rpx;
  align-items: center;
  justify-content: center;
}